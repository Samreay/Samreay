    "def one_hot_melt_pivot(df):\n",
    "    names = df.index.names\n",
    "    melted = df.reset_index().melt(id_vars=names)\n",
    "    return melted.pivot_table(index=names, \n",
    "                              columns=[\"variable\", \"value\"], \n",
    "                              aggfunc=len, \n",
    "                              fill_value=0)\n",
    "\n",
    "display(one_hot_melt_pivot(df))"
   ]
  }
