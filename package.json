{
    "name": "your.repo.name",
    "version": "0.0.1",
    "description": "Your repo description",
    "scripts": {
        "preinstall": "npx npm-force-resolutions",
        "setup": "gem install bundler && bundle install && npm i",
        "start": "concurrently --kill-others \"npm run css:dev\" \"bundle exec jekyll serve --config _config.yml,_config-dev.yml\"",
        "build": "npm run css:rel && jekyll build",
        "css:dev": "parcel watch styles/index.scss static/js/index.js --out-dir static --no-source-maps",
        "css:rel": "parcel build styles/index.scss static/js/index.js --out-dir static --no-source-maps --experimental-scope-hoisting"
    },
    "devDependencies": {
        "@fullhuman/postcss-purgecss": "^3.0.0",
        "@tailwindcss/forms": "^0.3.2",
        "autoprefixer": "^9.8.6",
        "concurrently": "^5.3.0",
        "parcel-bundler": "^1.12.5",
        "sass": "^1.37.5",
        "postcss-nested": "~>4.2.3",
        "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.4"
    },
    "dependencies": {
        "alpinejs": "^2.8.2",
        "aos": "^3.0.0-beta.6",
        "cruip-js-toolkit": "^1.1.2"
    },
    "resolutions": {
        "node-forge": "0.10.0"
    }
}
